{
    "name": "wotz/filament-table-filter-presets",
    "description": "Save and load named filter presets per user for Filament tables",
    "keywords": [
        "wotz",
        "laravel",
        "filament-table-filter-presets"
    ],
    "homepage": "https://github.com/wotz/filament-table-filter-presets",
    "license": "MIT",
    "authors": [
        {
            "name": "Who Owns The Zebra",
            "email": "dev@whoownsthezebra.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^5.0",
        "illuminate/contracts": "^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.16",
        "wotz/laravel-locale-collection": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\FilamentTableFilterPresets\\": "src/",
            "Wotz\\FilamentTableFilterPresets\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\FilamentTableFilterPresets\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\FilamentTableFilterPresets\\FilamentTableFilterPresetsServiceProvider"
            ],
            "aliases": {
                "FilamentTableFilterPresets": "Wotz\\FilamentTableFilterPresets\\Facades\\FilamentTableFilterPresets"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
